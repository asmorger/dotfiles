# Move windows to space. For some reason keys on bottom row of MacBook Pro don't work with this!
cmd + ctrl + shift - m : yabai -m window --space 1
cmd + ctrl + shift - n : yabai -m window --space  2; yabai -m space --focus 2
cmd + ctrl + shift - 0x2B : yabai -m window --space 3
cmd + ctrl + shift - 0x2F : yabai -m window --space 4

# Add current window to first window to make a stack
alt + ctrl + shift - i : yabai -m window --stack first

# Cycle through stack 
alt + ctrl + shift - u : yabai -m window --focus stack.prev
alt + ctrl + shift - j : yabai -m window --focus stack.next

# Toggles float mode for window. Hit twice to make a window leave a stack.
alt + ctrl + shift - f : yabai -m window --toggle float

# Move window focus using my vim positional keys 
alt + ctrl + shift - e : yabai -m window --focus east
alt + ctrl + shift - h : yabai -m window --focus west
alt + ctrl + shift - o : yabai -m window --focus north
alt + ctrl + shift - a : yabai -m window --focus south

# Move window position (swapping places)
cmd + alt + ctrl + shift - e : yabai -m window --swap east
cmd + alt + ctrl + shift - h : yabai -m window --swap west
cmd + alt + ctrl + shift - o : yabai -m window --swap north
cmd + alt + ctrl + shift - a : yabai -m window --swap south
